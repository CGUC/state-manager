{
  "compilerOptions": {
    "target": "ES2017",
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "src",
    "allowSyntheticDefaultImports": false,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false
    //   "noImplicitReturns": true,
    //   "noImplicitThis": true,
    //   "noImplicitAny": false,
  },
  "include": ["src/"],
  "exclude": ["node_modules", "jest"]
}
